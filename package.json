{
  "name": "nonnon",
  "version": "1.0.0",
  "description": "RSS-based article aggregation and content processing system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "start:compiled": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "diag:run": "tsx src/diagnostics/test_run.ts",
    "diag:scrape": "tsx src/diagnostics/test_scrape.ts",
    "diag:html": "tsx src/diagnostics/test_html_processing.ts",
    "diag:fetch": "tsx src/diagnostics/test_fetch_strategies.ts",
    "diag:all": "tsx src/diagnostics/test_all_sites.ts",
    "diag:save-html": "tsx src/diagnostics/save_html.ts",
    "db:update": "tsx src/diagnostics/update_database.ts",
    "db:reprocess": "tsx src/diagnostics/update_database.ts",
    "db:test-reprocess": "tsx src/diagnostics/test_reprocess.ts"
  },
  "keywords": [
    "scraper",
    "rss",
    "content-aggregation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@types/js-beautify": "^1.14.3",
    "cheerio": "^1.0.0-rc.12",
    "crawlee": "^3.15.1",
    "dotenv": "^16.3.1",
    "js-beautify": "^1.15.4",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/sanitize-html": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  }
}
